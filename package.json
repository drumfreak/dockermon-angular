{
  "name": "dockermon-angular",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "node ./bin/www",
    "dev": "concurrently -c \"green.bold\" -n \"SERVER\" \"nodemon --trace-warnings ./bin/www\"",
    "startlocal": "concurrently -c \"red.bold,green.bold\" -n \"SERVER,ANGULAR BUILD\" \"nodemon node app.js\" \"ng build --watch --output-path=./dist\"",
    "build": "ng build",
    "watch": "ng serve --watch --configuration=development",
    "test": "ng test",
    "docker": "docker-compose -f docker-compose.yml up",
    "docker:build": "docker-compose -f docker-compose.yml up --build"
  },
  "private": true,
